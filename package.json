{
  "name": "vue-pincher",
  "author": {
    "name": "Evgenii Ilin",
    "email": "ievgeniy88@gmail.com"
  },
  "description": "Vue 3 image cropper component",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ievgeniy88/vue-pincher/issues"
  },
  "homepage": "https://github.com/ievgeniy88/vue-pincher#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ievgeniy88/vue-pincher.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "image cropper",
    "picture cropper",
    "photo cropper",
    "image cropper",
    "cropper",
    "crop",
    "pan",
    "zoom",
    "pinch"
  ],
  "version": "0.2.3",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/vue-pincher.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/vue-pincher.js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "start": "vite",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "build": "vite build && vue-tsc -p tsconfig.lib.json",
    "prepublishOnly": "npm run build",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "prepare": "husky"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^22.16.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.17",
    "vue-tsc": "^2.2.10"
  }
}
